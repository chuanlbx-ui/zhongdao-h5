# 生产环境配置
NODE_ENV=production

# API配置
VITE_API_BASE_URL=https://api.zhongdao-mall.com/api/v1
VITE_SMS_API_KEY=production_sms_api_key_here

# 第三方短信服务商配置
VITE_SMS_PROVIDER=aliyun

# 阿里云短信配置（生产环境）
VITE_ALIYUN_ACCESS_KEY_ID=prod_aliyun_access_key_id
VITE_ALIYUN_ACCESS_KEY_SECRET=prod_aliyun_access_key_secret
VITE_ALIYUN_SIGN_NAME=中道商城
VITE_ALIYUN_TEMPLATE_CODE_LOGIN=SMS_PROD_LOGIN_123456
VITE_ALIYUN_TEMPLATE_CODE_REGISTER=SMS_PROD_REGISTER_123456

# 腾讯云短信配置
VITE_TENCENT_SECRET_ID=prod_tencent_secret_id
VITE_TENCENT_SECRET_KEY=prod_tencent_secret_key
VITE_TENCENT_APP_ID=987654321
VITE_TENCENT_SIGN_NAME=中道商城
VITE_TENCENT_TEMPLATE_ID_LOGIN=987654
VITE_TENCENT_TEMPLATE_ID_REGISTER=654321

# 调试配置
VITE_ENABLE_MOCK_SMS=false
VITE_DEBUG_SMS=false